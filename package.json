{
  "name": "dp-mine-dagpenger-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test": "vitest",
    "playwright": "npx playwright test",
    "playwright-ui": "npx playwright test --ui",
    "playwright-debug": "npx playwright test --debug",
    "playwright:verbose": "DEBUG=pw:api npx playwright test",
    "precommit-test": "vitest --run",
    "prepare": "husky",
    "setup-env": "cp .env.example .env",
    "generate-token": "chmod +x ./script/token-generator.sh && ./script/token-generator.sh",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.ts"
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.5.0",
    "@grafana/faro-web-sdk": "^1.12.2",
    "@grafana/faro-web-tracing": "^1.12.2",
    "@navikt/aksel-icons": "^7.9.1",
    "@navikt/ds-css": "^7.9.1",
    "@navikt/ds-react": "^7.9.1",
    "@navikt/nav-dekoratoren-moduler": "^3.1.1",
    "@navikt/oasis": "^3.6.0",
    "@portabletext/react": "^3.2.0",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@sanity/client": "^6.24.4",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "graphql-request": "^7.1.0",
    "groq": "^3.70.0",
    "html-react-parser": "^5.2.2",
    "isbot": "^5.1.21",
    "msw": "^2.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-typedjson": "^0.4.1",
    "tiny-invariant": "^1.3.3",
    "unleash-client": "^6.4.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "^4.5.1",
    "@graphql-codegen/typescript": "4.1.2",
    "@playwright/test": "^1.49.1",
    "@portabletext/types": "^2.0.13",
    "@remix-run/dev": "^2.15.2",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.19.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --list-different",
      "eslint --no-warn-ignored --max-warnings 0",
      "bash -c tsc"
    ]
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.24.2",
    "@rollup/rollup-linux-x64-gnu": "^4.30.1"
  }
}
